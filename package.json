{
  "name": "webgl-start",
  "version": "1.0.0",
  "description": "webgl practice",
  "main": "index.js",
  "scripts": {
    "hello-world":
      "glsl-server hello-world/index.js --dir hello-world --host 127.0.0.1 --title hello-world",
    "regl-hello-world":
      "glsl-server regl-hello-world/index.js --debug --host 127.0.0.1 --title regl-hello-world",
    "draw-points":
      "glsl-server draw-points/index.js --host 127.0.0.1 --title draw-points",
    "particles":
      "glsl-server particles/index.js --host 127.0.0.1 --title particles",
    "uniforms": "glsl-server uniforms"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Caged/webgl-start.git"
  },
  "keywords": ["webgl"],
  "author": "Justin Palmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Caged/webgl-start/issues"
  },
  "homepage": "https://github.com/Caged/webgl-start#readme",
  "dependencies": {
    "d3": "^4.12.2",
    "regl": "^1.3.0",
    "three": "^0.89.0"
  },
  "devDependencies": {
    "perspective-camera": "^2.0.1",
    "shader-reload": "^1.2.2"
  }
}
